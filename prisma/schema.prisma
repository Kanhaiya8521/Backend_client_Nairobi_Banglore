generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  first_name String
  mid_name String?
  last_name String
  image String?
  email String @unique
  phone String @unique
  password String
  social_link String?
  age Int?
  tob String?
}
